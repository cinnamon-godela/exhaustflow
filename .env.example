# Supabase – baseline chiller data (table: chiller_baseline)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your_publishable_key

# Chiller prediction API (AWS EC2). Default: http://3.16.135.140:8080
# Set to empty string to use Supabase for temperatures instead. No trailing slash.
VITE_CHILLER_API_URL=

# In dev, set to true to call the API via the Vite proxy (same origin). Avoids CORS; request goes from your machine to EC2.
VITE_CHILLER_USE_PROXY=true

# Gemini – design assistant chat (optional: set in .env to override)
VITE_GEMINI_API_KEY=your_gemini_api_key
